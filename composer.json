{
    "name": "yoannisj/craft-order-refunds",
    "description": "Detailed refunds for Craft Commerce orders",
    "type": "craft-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Yoannis Jamar",
            "homepage": "https://yoannis.com"
        }
    ],
    "support": {
        "email": "office@yoannis.com",
        "issues": "https://github.com/yoannisj/craft-order-refunds/issues?state=open",
        "source": "https://github.com/yoannisj/craft-order-refunds",
        "docs": "https://github.com/yoannisj/craft-order-refunds/blob/master/README.md"
    },
    "require": {
        "craftcms/cms": "^3.1.0",
        "craftcms/commerce": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
            "yoannisj\\orderrefunds\\": "src/"
        }
    },
    "extra": {
        "handle": "order-refunds",
        "name": "Order Refunds",
        "class": "yoannisj\\orderrefunds\\OrderRefunds"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^3.0",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.1",
        "codeception/specify": "^1.4",
        "codeception/assert-throws": "^1.1"
    }
}
